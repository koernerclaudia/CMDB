{
    "source": {
      "include": [""],             
      "exclude": ["node_modules"],    
      "includePattern": ".+\\.js(doc|x)?$", 
      "excludePattern": "(node_modules|\\.test\\.)" 
    },
    "opts": {
      "destination": "./out",        
      "recurse": true,                 
      "encoding": "utf8",               
      "readme": "README.md",            
      "force": true                    
    },
    "plugins": ["plugins/markdown"], 
    "tags": {
      "allowUnknownTags": true,        
      "dictionaries": ["jsdoc", "closure"] 
    }
  }
  